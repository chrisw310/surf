    function attachKeyboard(element) {
      if (typeof(VirtualKeyboard) != "undefined")
        VirtualKeyboard.attachInput(element);
    }
    $(function() {
      $("#email").on("focus", function() { attachKeyboard(this); });
      $("#password").on("focus", function() { attachKeyboard(this); });
  
