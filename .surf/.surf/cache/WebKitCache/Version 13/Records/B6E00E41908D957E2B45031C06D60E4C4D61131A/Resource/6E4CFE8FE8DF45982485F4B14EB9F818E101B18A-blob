    function getFederatedInfo() {
      var username = $('#email').val();
      FederatedLogin.isFederated(username, function(isFederated) {
        if (isFederated) {
          if (ext) {
            FederatedLogin.getPassword(username, function(password, authSessionId) {
              window.password.value = password;
              window.authsessionid.value = authSessionId;
              login();
            });
          } else {
            $('#rememberdiv').hide();
            $('#federatedalert').show();
          }
        } else {
          $('#rememberdiv').show();
          $('#federatedalert').hide();
        }
      });
    }

    
